{
  "name": "practice-storybook-vite",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "concurrently \"yarn:build:*\"",
    "build:storybook": "build-storybook",
    "build:tw": "yarn builz:vars && tailwindcss -i ./src/styles/tailwind.css -o ./public/tailwind.css",
    "builz:vars": "tsc -p tsconfig.vars.json",
    "dev": "concurrently \"yarn:dev:*\"",
    "dev:storybook": "start-storybook -p 6006",
    "dev:tw": "tailwindcss -i ./src/styles/tailwind.css -o ./public/tailwind.css -w",
    "dev:vars": "nodemon --watch ./src/styles/vars --ext ts --exec \"yarn builz:vars\"",
    "format": "eslint --fix ./src --ext .ts,.tsx,.js,.jsx --ignore-path .gitignore && prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "preview": "serve -l 3003 ./storybook-static",
    "vite:build": "tsc && vite build",
    "vite:dev": "vite",
    "vite:preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@radix-ui/react-checkbox": "^1.0.1",
    "@radix-ui/react-label": "^2.0.0",
    "@radix-ui/react-select": "^1.2.0",
    "@radix-ui/react-switch": "^1.0.1",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/builder-vite": "^0.3.0",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-tailwindcss": "^3.8.0",
    "lucide-react": "^0.105.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "react-merge-refs": "^2.0.1",
    "serve": "^14.1.2",
    "storybook-dark-mode": "^2.0.5",
    "tailwind-merge": "^1.8.1",
    "tailwindcss": "^3.2.4",
    "tailwindcss-animate": "^1.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.5"
  }
}
